class Node:
    def __init__(self, data):
        self.data = data
        self.next = None

# Create a LinkedList class
class LinkedList:
    def __init__(self):
        self.head = None

    def is_empty(self): 
        return self.head is None  
    def append(self, data):
        new_element=Node(data)
        if self.is_empty():
            self.head=new_element 
            self.tail=new_element 
        else:
            self.tail.next=new_element
            self.tail=new_element
    def __str__(self):
    #instance using print()
    #return string to be printed or print directly 
        result='[ '
        elementy = self.head
        while(elementy):
            result+=str(elementy.data)+", "
            elementy = elementy.next
        result+="]"
        print(result)
        return result

    def selectionSort(self): 
        temp = self.head
        # Traverse the List 
        while (temp): 
            minn = temp 
            r = temp.next
            # Traverse the unsorted sublist 
            while (r): 
                if (minn.data < r.data): 
                    minn = r 
                r = r.next
            # Swap Data 
            x = temp.data 
            temp.data = minn.data 
            minn.data = x 
            temp = temp.next
    # Method to add a node at begin of LL
